<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="#F4F4F4"
    tools:context=".FifthActivity"
    tools:ignore="ExtraText">



    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="55dp"
        android:layout_marginTop="58dp"
        android:fontFamily="@font/dm_sans_bold"
        android:text="Sign Up"
        android:textColor="#4A9C94"
        android:textSize="40sp" />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

    </LinearLayout>


    <Button
        android:id="@+id/sign_up_btn"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginLeft="32dp"
        android:backgroundTint="#FFAC41"
        android:textAlignment="center"
        android:textColorLink="#FFFFFF"
        tools:ignore="MissingPrefix"
        android:text="SIGN UP"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="240dp"/>


    <EditText
        android:id="@+id/userName"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView6"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:hint="User Name"
        android:inputType="text"
        android:textColor="#707070"
        android:textColorHint="#8E8E8E"
        tools:ignore="TouchTargetSizeCheck" />


    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/email"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/userName"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:hint="Email Address"
        android:inputType="textEmailAddress"
        android:textColor="#707070"
        android:textColorHint="#8E8E8E"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/password"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/email"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:inputType="textPassword"
        android:textColor="#707070"
        android:textColorHint="#8E8E8E"
        android:hint="Password"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="77dp"
        android:fontFamily="@font/dm_sans_medium"
        android:text="Have account?"
        android:textColor="#959595"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/login_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="44dp"
        android:fontFamily="@font/dm_sans_medium"
        android:text="Log in"
        android:textSize="20sp" />
>>>>>>> refs/remotes/origin/main

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        app:srcCompat="@drawable/message"
        android:layout_toLeftOf="@id/email"
        android:layout_below="@id/imageView86"
        android:layout_marginTop="44dp"/>

    <ImageView
        android:id="@+id/imageView86"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/profile"
        android:layout_toLeftOf="@id/userName"
        android:layout_marginTop="140dp"/>

    <ImageView
        android:id="@+id/imageView82"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/vector"
        android:layout_toLeftOf="@id/password"
        android:layout_below="@id/email"
        android:layout_marginTop="32dp"/>


</RelativeLayout>